{
  "name": "portfolio",
  "description": "Resume is a simple, yet elegant resume and CV theme using InfernoJs.",
  "version": "0.1.0",
  "repository": {
    "type": "git",
    "url": "https://github.com/adrijshikhar/adrijshikhar.github.io.git"
  },
  "dependencies": {
    "@babel/core": "7.12.3",
    "@svgr/webpack": "5.4.0",
    "axios": "^0.20.0",
    "babel-core": "7.0.0-bridge.0",
    "babel-eslint": "^10.1.0",
    "babel-jest": "26.3.0",
    "babel-loader": "8.1.0",
    "babel-preset-inferno-app": "^8.0.3",
    "bfj": "7.0.2",
    "bootstrap": ">=4.5.3",
    "case-sensitive-paths-webpack-plugin": "2.3.0",
    "chalk": "4.1.0",
    "css-loader": "5.0.0",
    "dotenv": "8.2.0",
    "dotenv-expand": "5.1.0",
    "eslint-config-prettier": "^6.15.0",
    "file-loader": "6.1.1",
    "fs-extra": "9.0.1",
    "html-webpack-plugin": "4.5.0",
    "identity-obj-proxy": "3.0.0",
    "inferno": "^7.4.5",
    "inferno-bootstrap": "^7.3.2",
    "inferno-dev-utils": "^6.0.4",
    "jest": "26.4.2",
    "jest-pnp-resolver": "1.2.2",
    "jest-resolve": "26.4.0",
    "mini-css-extract-plugin": "1.0.0",
    "node-forge": "^0.10.0",
    "node-sass": "^4.14.1",
    "optimize-css-assets-webpack-plugin": "5.0.4",
    "pnp-webpack-plugin": "1.6.4",
    "postcss-flexbugs-fixes": "4.2.1",
    "postcss-loader": "3.0.0",
    "postcss-preset-env": "6.7.0",
    "postcss-safe-parser": "5.0.2",
    "prettier": "^2.1.2",
    "react-app-polyfill": "^1.0.6",
    "resolve": "1.20.0",
    "sass-loader": "10.0.2",
    "style-loader": "1.2.1",
    "terser-webpack-plugin": "4.2.3",
    "url-loader": "4.1.1",
    "webpack": "4.44.2",
    "webpack-dev-server": "3.11.0",
    "webpack-manifest-plugin": "2.2.0",
    "workbox-webpack-plugin": "5.1.4"
  },
  "devDependencies": {
    "eslint": "7.7.0",
    "eslint-config-inferno-app": "^7.0.2",
    "eslint-loader": "4.0.2",
    "eslint-plugin-flowtype": "5.2.0",
    "eslint-plugin-import": "2.22.1",
    "eslint-plugin-inferno": "^7.20.1",
    "eslint-plugin-jsx-a11y": "6.3.1",
    "eslint-plugin-prettier": "^3.1.4",
    "gh-pages": "^3.1.0",
    "husky": "^4.3.0",
    "install": "^0.13.0",
    "lint-staged": "^10.4.0"
  },
  "license": "ISC",
  "scripts": {
    "start": "node scripts/start.js",
    "build": "node scripts/build.js",
    "deploy": "gh-pages -d build -b master",
    "lint": "eslint src --max-warnings 10000 --fix"
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ],
  "jest": {
    "collectCoverageFrom": [
      "src/**/*.{js,jsx}"
    ],
    "resolver": "jest-pnp-resolver",
    "setupFiles": [
      "react-app-polyfill/jsdom"
    ],
    "testMatch": [
      "<rootDir>/src/**/__tests__/**/*.{js,jsx}",
      "<rootDir>/src/**/?(*.)(spec|test).{js,jsx}"
    ],
    "testEnvironment": "jsdom",
    "testURL": "http://localhost",
    "transform": {
      "^.+\\.(js|jsx)$": "<rootDir>/node_modules/babel-jest",
      "^.+\\.css$": "<rootDir>/config/jest/cssTransform.js",
      "^(?!.*\\.(js|jsx|css|json)$)": "<rootDir>/config/jest/fileTransform.js"
    },
    "transformIgnorePatterns": [
      "[/\\\\]node_modules[/\\\\].+\\.(js|jsx)$",
      "^.+\\.module\\.(css|sass|scss)$"
    ],
    "moduleNameMapper": {
      "^react$": "inferno-compat",
      "^react-dom$": "inferno-compat",
      "^.+\\.module\\.(css|sass|scss)$": "identity-obj-proxy"
    },
    "moduleFileExtensions": [
      "web.js",
      "js",
      "json",
      "web.jsx",
      "jsx",
      "node"
    ]
  },
  "lint-staged": {
    "*.js": [
      "prettier --write",
      "eslint --fix"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  }
}
